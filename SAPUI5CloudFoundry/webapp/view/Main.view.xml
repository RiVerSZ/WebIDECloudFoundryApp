<mvc:View controllerName="be.wl.SAPUI5CloudFoundry.controller.Main" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<Page title="{i18n>title}">
		<content>
			<Toolbar>
				<Input value="{/item}"/><Button text="add item" press="onAddItem"/></Toolbar>
				<List items="{/items}">
					<StandardListItem title="{item}"></StandardListItem>
				</List>
		</content>
	</Page>
</mvc:View>